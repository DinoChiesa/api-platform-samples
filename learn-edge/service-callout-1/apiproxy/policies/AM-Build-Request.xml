<AssignMessage name="AM-Build-Request">
  <AssignTo createNew="true" type="request">myrequest</AssignTo>
  <IgnoreUnresolvedVariables>false</IgnoreUnresolvedVariables>
  <Set>
    <Path>/json</Path>
  </Set>
  <AssignVariable>
    <Name>target.copy.pathsuffix</Name>
    <Value>false</Value>
  </AssignVariable>
</AssignMessage>
